{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "strict": [2, "function"],
    "curly": [2, "multi-line"],
    "block-scoped-var": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-eq-null": 2,
    "eqeqeq": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "new-cap": [2, { "properties": false }],
    "operator-linebreak": [2, "after"],
    "camelcase": 0,
    "max-len": [2, 120],
    "quotes": [2, "single"],
    "no-multi-str": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "space-unary-ops": [2, {"nonwords": false, "overrides": {}}],
    "keyword-spacing": [2, {}],
    "space-infix-ops": 2,
    "space-before-blocks": [2, "always"],
    "eol-last": 2,
    "array-bracket-spacing": [2, "never", {"arraysInArrays": true}],
    "space-in-parens": [2, "never"],
    "no-multiple-empty-lines": 2,
    "wrap-iife": 2
  }
}